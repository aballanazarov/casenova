                                                    },
                                                    "title": {
                                                        "type": "string",
                                                        "format": "string",
                                                        "example": "Title Ru"
                                                    }
                                                },
                                                "type": "object",
                                                "format": "query"
                                            },
                                            "uz": {
                                                "properties": {
                                                    "name": {
                                                        "type": "string",
                                                        "format": "string",
                                                        "example": "Name Uz"
                                                    },
                                                    "title": {
                                                        "type": "string",
                                                        "format": "string",
                                                        "example": "Title Uz"
                                                    }
                                                },
                                                "type": "object",
                                                "format": "query"
                                            },
                                            "en": {
                                                "properties": {
                                                    "name": {
                                                        "type": "string",
                                                        "format": "string",
                                                        "example": "Name En"
                                                    },
                                                    "title": {
                                                        "type": "string",
                                                        "format": "string",
                                                        "example": "Title En"
                                                    }
                                                },
                                                "type": "object",
                                                "format": "query"
                                            }
                                        },
                                        "type": "object",
                                        "format": "query"
                                    }
                                },
                                "type": "object",
                                "collectionFormat": "multi"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful operation"
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
